<h1>sign in</h1>

<%=form_with url:sign_in_path do |form|%>
<div class= "mb-3">
</div>
<%= form.text_field :email, class:"form-control" %>
<div class="mb-3">
</div>
<%= form.text_field :password, class:"form-control" %>
<div class="mb-3">
</div>
<%= form.text_field :password_confirmation, class:"form-control" %>
<div class="mb-3">
</div>


<div class="mb-3">
<% form.submit%>
</div>

<% end %>